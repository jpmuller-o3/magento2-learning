<?xml version="1.0"?>
<schema xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:Setup/Declaration/Schema/etc/schema.xsd">
    <table name="onetree_final_price" resource="default">
        <column xsi:type="int" name="id" identity="true"/>
        <column xsi:type="varchar" name="customer_name" length="250" nullable="false"/>
        <column xsi:type="varchar" name="sku" length="30" nullable="false"/>
        <column xsi:type="decimal" name="final_price" nullable="false" scale="2" precision="10"/>
        <constraint xsi:type="primary" referenceId="PRIMARY">
            <column name="id"/>
        </constraint>
    </table>
</schema>
